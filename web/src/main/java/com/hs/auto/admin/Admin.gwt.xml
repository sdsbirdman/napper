<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.7.0//EN"
  "http://gwtproject.org/doctype/2.7.0/gwt-module.dtd">
<module rename-to='admin'>
	<inherits name='com.google.gwt.user.User' />
	<inherits name="com.github.gilbertotorrezan.gwtviews.gwtviews" />
	<inherits name="net.customware.gwt.dispatch.Dispatch" />
	<inherits name="com.google.gwt.inject.Inject" />
	<inherits name="com.google.common.base.Base" />
	<inherits name="com.google.common.collect.Collect" />
	
	<!-- Application Modules -->
	<inherits name="com.hs.auto.common.Common" />
	<inherits name="com.hs.auto.forums.Forums" />
	<inherits name="com.hs.auto.landing.Landing" />
	<inherits name="com.hs.auto.thread.Thread" />
	<inherits name="com.hs.auto.user.User" />
	<inherits name="com.hs.auto.oauth.OAuth" />
	

  	<!-- Specify the paths for translatable code -->
	<set-configuration-property name="CssResource.enableGss" value="true" />
	<source path='client' />
	<source path='shared' />

	<!-- allow Super Dev Mode -->
	<add-linker name="xsiframe" />
</module>
